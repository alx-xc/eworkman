<erl>

% shows ejobman job log as rss

out(A) ->
    File = ejobman_handler:get_job_log_filename(),
    case File of
        undefined ->
            {content, "text/plain", "Job log file name undefined."};
        _ ->
            Size = 8000,
            Text = ejobman_log:make_jlog_xml(File, Size),
            {content, "text/xml", Text}
    end.

</erl>
